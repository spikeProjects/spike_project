    <div id="tickets_view" class="{?gc.ticket_id}hide{/gc.ticket_id}"></div>
    <div class="bottom-wrapper">
        <div id="filter" class="hide"></div>
        <div class="bottom-container">
            <div id="ticketlist_footer" class="{^gc.ticket_id}hide {/gc.ticket_id}{?showServiceFeeInfo}service-fee {/showServiceFeeInfo}"></div>
        </div>
    </div>
    {>"/*@echo appNameVersioned*//dataready_error" /}
