<div id="event_ga_layout">
    <div id="ga_image">
        <div class="ga_promo_text">{@i18n key="event.common.ticket.ga"/}<p>{@i18n key="event.common.ticket.gapromo"/}</p></div>
    </div>
    <div class='ga spinner'><i class="sh-iconset sh-iconset-loader sh-animate-spin"></i></div>
    <div id="quantityfilter" class="hide"></div>
    {?gc.ticket_id}
        <div id="single-ticket">
            <div class='spinner'><i class="sh-iconset sh-iconset-loader sh-animate-spin"></i></div>
