<div id="event_ga_layout">
    <div id="ga_image">
        <div class="ga_promo_text">General Admission<p>All tickets give you access to seating/standing areas.</p></div>
    </div>
    <div class='ga spinner'><i class="sh-iconset sh-iconset-loader sh-animate-spin"></i></div>
    <div id="quantityfilter" class="hide"></div>
    {?gc.ticket_id}
        <div id="single-ticket">
            <div class='spinner'><i class="sh-iconset sh-iconset-loader sh-animate-spin"></i></div>
        </div>
    {/gc.ticket_id}

    <div id="tickets_view" {?gc.ticket_id}class="hide"{/gc.ticket_id}></div>
    <div class="bottom-wrapper">
        <div id="filter" class="hide"></div>
        <div class="bottom-container">
            <div id="ticketlist_footer" {^gc.ticket_id}class="hide"{/gc.ticket_id}></div>
            {@eq key=gc.country value="de"}
                <div id="serviceFee-ga" class="serviceFeeInfo">
                    <div class='sh-icons sh-info'></div>
                    <div class='infoContainer'>All prices are set by sellers, include VAT, may vary from face value and do not include service fees unless explicitly mentioned</div>
                </div>
            {/eq}
        </div>
    </div>
    {>"/*@echo appNameVersioned*//dataready_error" /}
</div>

{?hasBsfFeature}
<div id="bsf"></div>
{/hasBsfFeature}
<div id="eventDisclosure" class="hide"></div>
